// U10216012.cpp : 定義主控台應用程式的進入點。
//

#include "stdafx.h"

#include <stdio.h>
#include <stdlib.h>

int main(){
	int flour = 0;
	int milk = 0;
	int coco = 0;
	int waffle = 0;

	int ch_cookie = 0 ;
	int mi_cookie ;
	int wa_cookie = 0;
	int revenue ;
	int result = 0;

	scanf_s("%d%d%d%d", &flour, &milk, &coco, &waffle);

	
	for (mi_cookie = 1; mi_cookie <= flour || mi_cookie <= milk; ++mi_cookie){

		if (ch_cookie <= (flour - mi_cookie) || ch_cookie <= coco){
			ch_cookie++;
		}
		if (wa_cookie <= waffle || wa_cookie <= (milk - mi_cookie)){
			wa_cookie++;
		}
	
		revenue = ch_cookie * 80 + mi_cookie * 100 + wa_cookie * 60;
		
		if (result <= revenue){
			result = revenue;
		}
	}


	printf("%d %d %d\n %d\n", ch_cookie, mi_cookie, wa_cookie, result);


}

