// U10216012.cpp : 定義主控台應用程式的進入點。
//

#include "stdafx.h"

#include <stdio.h>
#include <stdlib.h>

int main(){
	int flour = 0;
	int milk = 0;
	int coco = 0;
	int waffle = 0;

	int ch_cookie = 0;
	int mi_cookie = 0;
	int wa_cookie = 0;
	int revenue = 0;
	int result = 0;
	size_t i;
	size_t j;
	size_t k;


	scanf("%d%d%d%d", &flour, &milk, &coco, &waffle);

	
	for (i = mi_cookie; i <= flour || i <= milk; ++i){
		for (j = ch_cookie; j <= (flour - i) || j <= coco; ++j){
		}
		for (k = wa_cookie; k <= waffle || k <= (milk - i); ++k){
		}
		revenue = ch_cookie * 80 + mi_cookie * 100 + wa_cookie * 60;
		
		if (result <= revenue){
			result = revenue;
		}
	}


	printf("%d %d %d\n %d\n", ch_cookie, mi_cookie, wa_cookie, result);


}

