#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <ctype.h>
#define SIZE 1000


int main(){
  size_t i ;
  size_t j ;
  int a = 0;
  char sentence[SIZE];
  char alpha[SIZE]
  char alphas[SIZE];
  
  fgets(sentence, SIZE, stdin);
  
  for(i = 0 ;i < SIZE; i++){
    if(isalpha( sentence[i] )){
     alpha[a++] = sentence[i];
    }
  }
  
  for(j = 0 ;j < SIZE; j++){
     alphas[j] = toupper( alpha[j] );
  }
  

system('pause');
}


